{
  "name": "mongo-studio",
  "version": "1.0.0",
  "private": true,
  "description": "A lightweight MongoDB GUI tool similar to Prisma Studio",
  "main": "dist/server/index.js",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start": "node dist/server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["mongodb", "gui", "mongoose", "typescript", "database-tool"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mongoose": "^8.10.0",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^1.12.3"
  },
  "packageManager": "pnpm@8.15.1"
}
